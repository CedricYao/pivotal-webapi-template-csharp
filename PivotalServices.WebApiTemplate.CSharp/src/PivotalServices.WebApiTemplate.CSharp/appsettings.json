{
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  },
  "spring": {
    "application": {
      "name": "PivotalServices.WebApiTemplate.CSharp"
    },
    "cloud": {
      "config": {
        "validate_certificates": false,
        "failFast": false,
        "name": "PivotalServices.WebApiTemplate.CSharp",
        "env": "${ASPNETCORE_ENVIRONMENT}"
      }
    }
  },
  "eureka": {
    "client": {
      "validate_certificates": false
    },
    "instance": {
      "nonSecurePortEnabled": false,
      "securePortEnabled": true
    }
  }
}